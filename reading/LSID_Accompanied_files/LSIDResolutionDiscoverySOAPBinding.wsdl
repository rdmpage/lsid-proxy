<definitions xmlns:tns="http://www.omg.org/LSID/2003/ResolutionDiscovery/StandardSOAPBinding"
		targetNamespace="http://www.omg.org/LSID/2003/ResolutionDiscovery/StandardSOAPBinding"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		xmlns="http://schemas.xmlsoap.org/wsdl/"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
		xmlns:sns="http://www.omg.org/LSID/2003/Standard/ResolutionDiscovery/WSDL">

	<import namespace="http://www.omg.org/LSID/2003/Standard/ResolutionDiscovery/WSDL" location="LSIDResolutionDiscoveryPortType.wsdl" />

	<binding name="LSIDResolutionDiscoverySOAPBinding" type="sns:LSIDResolutionDiscoveryServicePortType">
		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http" />
		<operation name="getLSIDResolutionService">
			<soap:operation />
			<input>
				<soap:body use="literal" namespace="http://www.omg.org/LSID/2003/ResolutionDiscovery/StandardSOAPBinding" />
			</input>
			<output>
				<soap:body use="literal" namespace="http://www.omg.org/LSID/2003/ResolutionDiscovery/StandardSOAPBinding" />
			</output>
		</operation>
	</binding>

</definitions>
